{
  "name": "whatsapp-clone",
  "version": "1.0.0",
  "description": "WhatsApp Web Clone with real-time messaging",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "postinstall": "cd client && (npm ci || npm install)",
    "start": "node server/index.js",
    "process:webhooks": "node server/scripts/processWebhooks.js"
  },
  "keywords": ["whatsapp", "clone", "real-time", "chat"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "socket.io": "^4.7.2",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  }
} 